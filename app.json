{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro",
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          }
        ]
      ]
    },
    "splash": {
      "image": "./app/assets/images/splash_screens/splash.png"
    },
    "name": "Polaris",
    "slug": "Polaris",
    "android": {
      "package": "com.cliftonchee.Polaris"
    },
    "ios": {
      "bundleIdentifier": "com.cliftonchee.Polaris"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "82301719-c33f-4139-b161-224ca1058f36"
      }
    }
  }
}
